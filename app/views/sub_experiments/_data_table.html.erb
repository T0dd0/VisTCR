<div class="textbox">
  <div class = "textbox-title">
    <div>Data details
    </div>  
  </div>
  <div class="textbox-content">
    <div style="height:300px;overflow:auto">
      <table class="table table-hover table-condensed">
        <thead>
          <tr>
            <% @data[:head].each do |s| %>
              <th> <%= s %></th>
            <% end %>
          </tr>
        </thead>
        <tbody>
          <% @data[:body].each do |dd| %>
            <tr>
              <% dd.each do |d| %>
                <td> <%= d %> </td>
              <% end %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<div>
  <button class="btn btn-mini pull-right" onclick= "<%= @url_download %>"> Download Data</button>
</div>


