<div class="textbox">
  <div class = "textbox-title"><b><%= @detector_name  %></b>  </div>
  <div class="splitline"> </div>
  <div class="textbox-content">
    <table class='table  table-hover table-condensed'>
      <thead>
        <tr>
          <th>Sample</th>
          <th>Ct</th>
          <th>Conf</th>
          <th></th>
        </tr>
      </thead>
      <tbody>  
        <%  @channels.each do |ch| %>
        <tr>
          <td><%=ch.sample%></td>
          <td><%=ch.ct_value%></td>
          <td><%=ch.confidence%></td>
          <td><button class="btn btn-mini" onclick = "edit_channel_conf(<%= ch.id %>)">Set Conf</button></td>
        </tr>
      <% end %> 
      </tbody>
      </div>
    </div>


