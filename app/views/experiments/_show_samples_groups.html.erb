<div class="panel panel-default">
  <div class="panel-heading"><%= raw @groups[:title] %> </div>
  <div class="panel-body">
 
  <table class="table table-hover table-condensed">
    <thead>
      <tr>
        <% @groups[:head].each_with_index do |s| %>
          <th  > <%= s %> </th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @groups[:body].each do |dd| %>
      <tr>
        <% dd.each_with_index do |d,i| %>
          <td > <%= d %> </td>
        <% end %>
      </tr>
    <% end %>
   </tbody>
  </table>
 </div>
 </div>
